{
  "name": "basicapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && npm cache clean --force",
    "clean:android": "cd android/ && ./gradlew clean && cd .. && react-native run-android",
    "newclear": "rm -rf $TMPDIR/react-* && watchman watch-del-all && rm -rf ios/build && rm -rf node_modules/ && npm cache clean --force && npm i",
    "test:watch": "jest --watch",
    "updateSnapshot": "jest --updateSnapshot",
    "coverage": "jest --coverage && open coverage/lcov-report/index.html || xdg-open coverage/lcov-report/index.html",
    "android:build": "cd android && ./gradlew assembleRelease",
    "android:install": "cd android && ./gradlew assembleRelease && ./gradlew installRelease",
    "android:hockeyapp": "cd android && ./gradlew assembleRelease && puck -submit=auto app/build/outputs/apk/app-release.apk",
    "android:devices": "$ANDROID_HOME/platform-tools/adb devices",
    "android:logcat": "$ANDROID_HOME/platform-tools/adb logcat *:S ReactNative:V ReactNativeJS:V",
    "android:shake": "$ANDROID_HOME/platform-tools/adb devices | grep '\\t' | awk '{print $1}' | sed 's/\\s//g' | xargs -I {} $ANDROID_HOME/platform-tools/adb -s {} shell input keyevent 82",
    "lint": "standard --verbose | snazzy",
    "lintdiff": "git diff --name-only --cached --relative | grep '\\.js$' | xargs standard | snazzy",
    "fixcode": "standard --fix",
    "git-hook": "npm run lint -s && npm run test -s",
    "fix-icons": "rm ./node_modules/react-native/local-cli/core/__fixtures__/files/package.json",
    "react-devtools": "react-devtools",
    "postinstall": "rm -f ./node_modules/react-native/local-cli/core/__fixtures__/files/package.json",
    "appium:doctor": "appium-doctor",
    "start:appium": "appium",
    "test:e2e": "jest --testMatch=\"**/__tests__/**/*e2e.js\"",
    "test:e2e:android": "E2E_DEVICE=\"android-emulator\" npm run test:e2e",
    "test:e2e:ios": "E2E_DEVICE=\"ios-emulator\"  npm run test:e2e"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.2",
    "apisauce": "^0.14.2",
    "appcenter": "^1.10.0",
    "appcenter-analytics": "^1.10.0",
    "appcenter-crashes": "^1.10.0",
    "lodash": "^4.17.4",
    "metro-react-native-babel-preset": "^0.53.0",
    "moment": "^2.21.0",
    "prop-types": "^15.6.0",
    "ramda": "^0.25.0",
    "react": "16.6.3",
    "react-native": "0.57.8",
    "react-native-animatable": "^1.2.4",
    "react-native-app-settings": "^2.0.1",
    "react-native-config": "^0.11.6",
    "react-native-custom-actionsheet": "^0.0.4",
    "react-native-exception-handler": "^2.9.0",
    "react-native-exit-app": "^1.0.0",
    "react-native-fast-image": "^5.0.10",
    "react-native-fbsdk": "^0.8.0",
    "react-native-firebase": "^5.0.0",
    "react-native-fs": "^2.13.3",
    "react-native-geolocation-service": "^2.0.0",
    "react-native-gifted-chat": "^0.7.2",
    "react-native-google-maps": "^1.0.0",
    "react-native-image-crop-picker": "^0.21.2",
    "react-native-indicators": "^0.12.0",
    "react-native-keyboard-aware-scroll-view": "^0.5.0",
    "react-native-linear-gradient": "^2.4.0",
    "react-native-maps": "^0.23.0",
    "react-native-nav": "^2.0.2",
    "react-native-navigation": "xvonabur/react-native-navigation",
    "react-native-permissions": "^1.1.1",
    "react-native-push-notification": "^3.1.2",
    "react-native-share": "1.1.0",
    "react-native-shortcut-badge": "^0.1.0-beta.2",
    "react-native-snap-carousel": "^3.7.0",
    "react-native-splash-screen": "^3.1.1",
    "react-native-svg": "^6.3.1",
    "react-native-system-setting": "^1.7.1",
    "react-native-vector-icons": "^4.6.0",
    "react-redux": "^5.0.6",
    "reactotron-react-native": "^3.2.1",
    "redux": "^3.7.2",
    "redux-form": "^7.3.0",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.10.0",
    "redux-persist-filesystem-storage": "^1.3.1",
    "redux-saga": "^0.16.0",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "rollbar-react-native": "^0.5.0",
    "seamless-immutable": "^7.1.3",
    "webdriverio": "^4.14.1",
    "xdate": "^0.8.2"
  },
  "devDependencies": {
    "babel-core": "7.0.0-beta.3",
    "babel-eslint": "8.2.3",
    "babel-jest": "24.7.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react-native": "^5",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "metro-react-native-babel-preset": "0.53.1",
    "react-test-renderer": "16.6.3",
    "eslint-config-react-app": "^2.1.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-flowtype": "^2.46.3",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-react-native": "^3.2.1",
    "eslint-plugin-standard": "^3.0.1",
    "husky": "^0.14.3",
    "jest": "24.7.1",
    "mockery": "^2.1.0",
    "react-dom": "16.0.0",
    "snazzy": "^7.1.1",
    "standard": "^11.0.1",
    "wd": "^1.11.1"
  },
  "jest": {
    "preset": "react-native"
  },
  "config": {},
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "describe",
      "test",
      "jest",
      "expect",
      "fetch",
      "navigator",
      "__DEV__",
      "XMLHttpRequest",
      "FormData",
      "React$Element"
    ]
  }
}
